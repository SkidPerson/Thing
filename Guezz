    ["Silly guitar.txt"] = [[
-- Silly Guitar LUA
-- made by Larkinian















local ToolName = "Guitar"

local holdAnimationId = "rbxassetid://55789536"
local useAnimationId = "rbxassetid://55791140"

local soundIds = {
	"rbxassetid://82958924923926",
	"rbxassetid://95378365685348"
}

local assetId = "rbxassetid://123281101233950"

local iconId = "rbxassetid://94238506858484"

local cooldownTime = 1.5
local canUse = true

local function giveGuitar()
	local player = game.Players.LocalPlayer
	
	local t = Instance.new("Tool")
	t.Name = ToolName
	t.RequiresHandle = true
	t.CanBeDropped = false
	
	t.TextureId = iconId
	
	t.Grip =
		CFrame.new(0.3, -0.6, -0.2) *
		CFrame.Angles(0, 190, math.rad(0))

	t.Parent = player.Backpack

	local objects = game:GetObjects(assetId)
	if objects and objects[1] then
		local assetModel = objects[1]
		assetModel.Parent = t
		
		if not t:FindFirstChild("Handle") then
			local firstPart = assetModel:FindFirstChildWhichIsA("BasePart", true)
			if firstPart then
				firstPart.Name = "Handle"
				firstPart.Parent = t
			end
		end
	end

	for _, obj in pairs(t:GetDescendants()) do
		if obj:IsA("BasePart") then
			obj.CanCollide = false
			obj.Massless = true
			obj.Anchored = false
		end
	end

	local remote = Instance.new("RemoteEvent")
	remote.Name = "GuitarRemote"
	remote.Parent = t

	local holdAnimation
	local useAnimation

	local function playAnimation(player, hold, use)
		local character = player.Character
		if not character then return end
		
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if not humanoid then return end
		
		local animator = humanoid:FindFirstChildOfClass("Animator")
		if not animator then
			animator = Instance.new("Animator")
			animator.Parent = humanoid
		end

		if hold then
			if not holdAnimation then
				local animation = Instance.new("Animation")
				animation.AnimationId = holdAnimationId
				holdAnimation = animator:LoadAnimation(animation)
				holdAnimation.Looped = true
				holdAnimation:Play()
			end
		else
			if holdAnimation then
				holdAnimation:Stop()
				holdAnimation = nil
			end
		end

		if use then
			local animation = Instance.new("Animation")
			animation.AnimationId = useAnimationId
			useAnimation = animator:LoadAnimation(animation)
			useAnimation.Looped = false
			useAnimation:Play()
		end
	end
	remote.OnClientEvent:Connect(function(soundId, position)
		local sound = Instance.new("Sound")
		sound.SoundId = soundId
		sound.Volume = 1
		sound.RollOffMaxDistance = 50
		sound.Parent = workspace
		sound.Position = position
		sound:Play()
		
		game.Debris:AddItem(sound, 5)
	end)

	t.Equipped:Connect(function()
		playAnimation(player, true, false)
	end)

	t.Unequipped:Connect(function()
		playAnimation(player, false, false)
	end)

	t.Activated:Connect(function()
		if not canUse then return end
		canUse = false
		
		playAnimation(player, true, true)

		local selectedSoundId = soundIds[math.random(#soundIds)]
		
		remote:FireServer(selectedSoundId)

		task.wait(cooldownTime)
		canUse = true
	end)
end

giveGuitar()
    ]],


}

local normalColor = Color3.fromRGB(255,255,255)
local normalTextColor = Color3.fromRGB(0,0,0)
local hoverClickColor = Color3.fromRGB(0,135,255)
local hoverTextColor = Color3.fromRGB(255,255,255)

for i = 1, #files do
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, -6, 0, buttonHeight)
    btn.Position = UDim2.new(0, 3, 0, (i-1) * (buttonHeight + padding))
    
    btn.BackgroundColor3 = normalColor
    btn.BorderSizePixel = 0
    btn.TextXAlignment = Enum.TextXAlignment.Left
    btn.Text = files[i]
    btn.TextSize = 9
    btn.TextColor3 = normalTextColor
    btn.Parent = scrolling
    
    btn.MouseEnter:Connect(function()
        btn.BackgroundColor3 = hoverClickColor
        btn.TextColor3 = hoverTextColor
    end)
    btn.MouseLeave:Connect(function()
        btn.BackgroundColor3 = normalColor
        btn.TextColor3 = normalTextColor
    end)
    btn.MouseButton1Down:Connect(function()
        btn.BackgroundColor3 = hoverClickColor
        btn.TextColor3 = hoverTextColor
    end)
    btn.MouseButton1Up:Connect(function()
        btn.BackgroundColor3 = normalColor
        btn.TextColor3 = normalTextColor
    end)
    
    btn.MouseButton1Click:Connect(function()
        codeBox.Text = codes[files[i]]
    end)
end

scrolling.CanvasSize = UDim2.new(0, 0, 0, #files * (buttonHeight + padding))
local minimized = false
local normalSize = mainFrame.Size

minimizeButton.MouseButton1Click:Connect(function()
	minimized = not minimized
	
	if minimized then
		
		for _, obj in ipairs(mainFrame:GetChildren()) do
			if obj:IsA("GuiObject") and obj ~= topbar then
				obj.Visible = false
			end
		end
		
		mainFrame.Size = UDim2.new(
			normalSize.X.Scale,
			normalSize.X.Offset,
			0,
			topbar.Size.Y.Offset
		)
	else
		for _, obj in ipairs(mainFrame:GetChildren()) do
			if obj:IsA("GuiObject") then
				obj.Visible = true
			end
		end
		
		mainFrame.Size = normalSize
	end
end)
