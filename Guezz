settingsButton.Position = settingsUp

executorButton.Size = UDim2.new(  
	executorOriginalSize.X.Scale,  
	executorOriginalSize.X.Offset,  
	executorOriginalSize.Y.Scale,  
	executorOriginalSize.Y.Offset - shrink  
)  
