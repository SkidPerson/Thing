    ["Barbie Car.txt"] = [[
-- Barbie Car LUA
-- i'am a barbie girl :3
-- made by Larkinian
















 

local assetId = "rbxassetid://10146407482"  

local model = game:GetObjects(assetId)[1]  
if not model then  
    warn("incorrect model bitch")  
    return  
end  

local Players = game:GetService("Players")  
local plr = Players.LocalPlayer  
local character = plr.Character or plr.CharacterAdded:Wait()  
local hrp = character:WaitForChild("HumanoidRootPart")  

local primary = model:FindFirstChildWhichIsA("BasePart", true)  
if not primary then  
    warn("none BasePart found")  
    return  
end  
model.PrimaryPart = primary  

for _, part in ipairs(model:GetDescendants()) do  
    if part:IsA("BasePart") and part ~= model.PrimaryPart then  
        local weld = Instance.new("WeldConstraint")  
        weld.Part0 = model.PrimaryPart  
        weld.Part1 = part  
        weld.Parent = model.PrimaryPart  
    end  
end  

for _, part in ipairs(model:GetDescendants()) do  
    if part:IsA("BasePart") then  
        part.Anchored = false  
        part.CanCollide = true 
        part.Massless = true
    end  
end  

model.Parent = workspace  

local offset = CFrame.new(7, -1.8, 7)  
local rotation = CFrame.Angles(0, -190, math.rad(-90))  
local finalCFrame = hrp.CFrame * offset * rotation  
model:PivotTo(finalCFrame)  

local weldToPlayer = Instance.new("WeldConstraint")  
weldToPlayer.Part0 = hrp  
weldToPlayer.Part1 = model.PrimaryPart  
weldToPlayer.Parent = hrp  

for _, part in ipairs(model:GetDescendants()) do
    if part:IsA("BasePart") then
        part.Touched:Connect(function(hit)
            local h = hit.Parent:FindFirstChildOfClass("Humanoid")
            local p = Players:GetPlayerFromCharacter(hit.Parent)
            if h and p and p ~= plr then
                
                h.Health = 0
                
                local hrpTarget = hit.Parent:FindFirstChild("HumanoidRootPart")
                if hrpTarget then
                    hrpTarget.Velocity = (hrpTarget.Position - hrp.Position).Unit * 200 + Vector3.new(0,100,0)
                end
            end
        end)
    end
end

local humanoid = character:WaitForChild("Humanoid")
for _, partName in pairs({"Left Leg", "Right Leg", "LeftLowerLeg", "RightLowerLeg", "LeftFoot", "RightFoot"}) do  
    local part = character:FindFirstChild(partName)  
    if part then  
        part.Transparency = 1  
        part.CanCollide = false  
    end  
end  

local animator = humanoid:FindFirstChildOfClass("Animator")  
if animator then  
    animator:Destroy()  
end  

humanoid.JumpPower = 0
humanoid.JumpHeight = 0

local UserInputService = game:GetService("UserInputService")  
local RunService = game:GetService("RunService")  

local speed = 16
local direction = Vector3.new(0,0,0)  

UserInputService.InputBegan:Connect(function(input, processed)  
    if processed then return end  
    if input.KeyCode == Enum.KeyCode.W then  
        direction = Vector3.new(0,0,-1)  
    elseif input.KeyCode == Enum.KeyCode.S then  
        direction = Vector3.new(0,0,1)  
    elseif input.KeyCode == Enum.KeyCode.A then  
        direction = Vector3.new(-1,0,0)  
    elseif input.KeyCode == Enum.KeyCode.D then  
        direction = Vector3.new(1,0,0)  
    end  
end)  

UserInputService.InputEnded:Connect(function(input)  
    if input.KeyCode == Enum.KeyCode.W or input.KeyCode == Enum.KeyCode.S  
    or input.KeyCode == Enum.KeyCode.A or input.KeyCode == Enum.KeyCode.D then  
        direction = Vector3.new(0,0,0)  
    end  
end)  

RunService.RenderStepped:Connect(function(delta)  
    if direction.Magnitude > 0 then  
        local move = (hrp.CFrame:VectorToWorldSpace(direction) * speed * delta)  
        hrp.CFrame = hrp.CFrame + Vector3.new(move.X,0,move.Z)  
    end  
end)  

humanoid.WalkSpeed = 300

local attachment = Instance.new("Attachment")
attachment.Parent = hrp
attachment.Position = Vector3.new(0, -2, -18)

local spot = Instance.new("SpotLight")
spot.Parent = attachment
spot.Color = Color3.fromRGB(255,255,255)
spot.Brightness = 5
spot.Range = 20
spot.Angle = 90
spot.Face = Enum.NormalId.Top
spot.Shadows = true
spot.Enabled = true
    ]],
    ["Audio visualizer.txt"] = [[
-- audio visualizer LUA
-- idk if this still works
















 

loadstring(game:HttpGet("https://pastefy.app/ongq4c6T/raw?part=Autovisualizer%20Ring%20By%20Luaman", true))()
    ]],

