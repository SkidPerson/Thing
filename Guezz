local player = game.Players.LocalPlayer
local outputLabel = LMG2L["TextLabel_b"]

outputLabel.TextWrapped = true
outputLabel.TextYAlignment = Enum.TextYAlignment.Top
outputLabel.TextXAlignment = Enum.TextXAlignment.Left
outputLabel.Text = ""

local function addOutput(text)
    if outputLabel.Text == "" then

        outputLabel.Text = "[OutPut] : " .. text
    else

        outputLabel.Text = outputLabel.Text .. "\n[OutPut] : " .. text
    end
end

addOutput("welcome to YPRE " .. player.Name)

executeButton.MouseButton1Click:Connect(function()
    local code = codeBox.Text
    if code ~= "" then
        addOutput("Executed successfully")
    else
        addOutput("write something bru")
    end
end)

clearButton.MouseButton1Click:Connect(function()
    local text = codeBox.Text:gsub("%s+", "")
    if text == "" then
        addOutput("text box cleared successfully")
    else
        codeBox.Text = ""
        addOutput("text box cleared successfully")
    end
end)

