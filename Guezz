    ["Epstein music.txt"] = [[
-- Epstein music LUA
-- "could u please give us your name?" "Jeffrey Epstein"
-- It was originally supposed to be a face script, but the decal was banned so I had to just use the music lol
-- made by Larkinian















 


local Players = game:GetService("Players")
local SoundService = game:GetService("SoundService")

local SOUND_ID = "rbxassetid://75856483086633"

local sound = SoundService:FindFirstChild("GlobalSound")
if not sound then
	sound = Instance.new("Sound")
	sound.Name = "GlobalSound"
	sound.SoundId = SOUND_ID
	sound.Looped = true
	sound.Volume = 1500
	sound.PlaybackSpeed = 0.12
	sound.Parent = SoundService

	local eq = Instance.new("EqualizerSoundEffect")
	eq.LowGain = -17
	eq.MidGain = -2
	eq.HighGain = 1
	eq.Parent = sound

	local comp = Instance.new("CompressorSoundEffect")
	comp.Threshold = -24
	comp.Ratio = 10
	comp.Attack = 0.03
	comp.Release = 0.15
	comp.Parent = sound

	sound:Play()
end


local IMAGE_ID = "http://www.roblox.com/asset/?id=109767059488315"

local function aplicar(player, character)
	local head = character:WaitForChild("Head", 5)
	if not head then return end

	if head:FindFirstChild("HeadGui") then
		head.HeadGui:Destroy()
	end

	head.Transparency = 1
	head.CanCollide = false

	local gui1 = Instance.new("BillboardGui")
	gui1.Name = "HeadGui"
	gui1.Parent = head
	gui1.Adornee = head
	gui1.Size = UDim2.new(2.5, 0, 2.5, 0)
	gui1.StudsOffset = Vector3.new(0, 0.2, 0)
	gui1.AlwaysOnTop = true

	local img = Instance.new("ImageLabel")
	img.Name = "Image"
	img.Image = IMAGE_ID
	img.Size = UDim2.new(1.6, 0, 1.6, 0)
	img.Position = UDim2.new(-0.3, 0, -0.3, 0)
	img.BackgroundTransparency = 1
	img.Parent = gui1
end

for _, player in ipairs(Players:GetPlayers()) do
	if player.Character then
		aplicar(player, player.Character)
	end
	player.CharacterAdded:Connect(function(char)
		aplicar(player, char)
	end)
end

Players.PlayerAdded:Connect(function(player)
	player.CharacterAdded:Connect(function(char)
		aplicar(player, char)
	end)
end)
    ]],


