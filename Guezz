    ["A-60.txt"] = [[
-- A-60 LUA
-- made by Larkinian
















 

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local Player = Players.LocalPlayer
local char = Player.Character or Player.CharacterAdded:Wait()
local HRP = char:WaitForChild("HumanoidRootPart")
local Humanoid = char:WaitForChild("Humanoid")

local imageIds = {
    79410079461565, 102294381496489, 126304908663818, 82183923736560, 122272293623590
}

for _, part in pairs(char:GetDescendants()) do
    if part:IsA("BasePart") then
        part.Transparency = 1
        part.CanCollide = false
    elseif part:IsA("Accessory") then
        part.Handle.Transparency = 1
    end
end

local Billboard = Instance.new("BillboardGui", HRP)
Billboard.Size = UDim2.new(7,0,7,0)
Billboard.AlwaysOnTop = true

local ImageLabel = Instance.new("ImageLabel", Billboard)
ImageLabel.Size = UDim2.new(1,0,1,0)
ImageLabel.BackgroundTransparency = 1
ImageLabel.Image = "http://www.roblox.com/asset/?id=" .. imageIds[1]

local light = Instance.new("PointLight", HRP)
light.Color = Color3.fromRGB(255,0,0)
light.Brightness = 5
light.Range = 15

spawn(function()
    while true do
        for _, id in ipairs(imageIds) do
            ImageLabel.Image = "http://www.roblox.com/asset/?id=" .. id
            wait(0.03)
        end
        ImageLabel.Image = ""
        wait(0.6)
    end
end)

RunService.RenderStepped:Connect(function()
    Billboard.CFrame = HRP.CFrame
end)

Humanoid.WalkSpeed = 300

local FALL_LIMIT = -50
RunService.Heartbeat:Connect(function()
    if HRP.Position.Y < FALL_LIMIT then
        HRP.CFrame = CFrame.new(0,50,0)
    end
end)

local touchPart = Instance.new("Part")
touchPart.Size = Vector3.new(7,7,7)
touchPart.Transparency = 1
touchPart.CanCollide = false
touchPart.Anchored = true
touchPart.CFrame = HRP.CFrame
touchPart.Parent = workspace

RunService.RenderStepped:Connect(function()
    touchPart.CFrame = HRP.CFrame
end)

touchPart.Touched:Connect(function(hit)
    local otherChar = hit.Parent
    local otherHumanoid = otherChar:FindFirstChild("Humanoid")
    local otherPlayer = Players:GetPlayerFromCharacter(otherChar)
    
    if otherHumanoid and otherPlayer ~= Player then
        otherHumanoid.Health = 0

        local root = otherChar:FindFirstChild("HumanoidRootPart")
        if root then
            root.Velocity = Vector3.new(math.random(-50,50),100,math.random(-50,50))
        end
    end
end)
