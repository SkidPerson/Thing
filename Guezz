local executorOriginalSize = executorButton.Size
local executorOriginalPos = executorButton.Position
local tabExecutorPos = tabLine.Position

local activeColor = Color3.fromRGB(255, 255, 255)
local inactiveColor = Color3.fromRGB(243, 243, 243)

