    ["Obunga.txt"] = [[
-- obunga LUA
-- yo, obama
-- made by Larkinian
















 

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")


local function typeHint(text, speed, stayTime)
	local hint = Instance.new("Hint")
	hint.Parent = Workspace
	hint.Text = ""

	for i = 1, #text do
		hint.Text = string.sub(text, 1, i)
		task.wait(speed)
	end

	task.wait(stayTime)
	hint:Destroy()
end

typeHint(
	"Robloxia has started a lockdown, please run away and hide.",
	0.05,
	4
)

task.wait(0)

typeHint(
	"He's here.",
	0.08,
	3
)


local Chat_ = Instance.new("ScreenGui")
Chat_.Name = "Chat_"
Chat_.DisplayOrder = 999999999
Chat_.ResetOnSpawn = false
Chat_.IgnoreGuiInset = true

local obama = Instance.new("ImageLabel")
obama.Name = "obama"
obama.Parent = Chat_
obama.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
obama.Size = UDim2.new(1, 0, 1, 0)
obama.Visible = false
obama.Image = "rbxassetid://5643866"

local obunga = Instance.new("ImageLabel")
obunga.Name = "obunga"
obunga.Parent = Chat_
obunga.BackgroundTransparency = 1
obunga.Size = UDim2.new(1, 0, 1, 0)
obunga.Image = "rbxassetid://15603607126"
obunga.ImageTransparency = 1

local help = Instance.new("ImageLabel")
help.Name = "help"
help.Parent = Chat_
help.BackgroundTransparency = 1
help.Size = UDim2.new(1, 0, 1, 0)
help.Image = "rbxassetid://2050833946"
help.ImageTransparency = 1

local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = Chat_
TextLabel.BackgroundTransparency = 1
TextLabel.Visible = false
TextLabel.Size = UDim2.new(1, 0, 0.065, 0)

local nope = Instance.new("Sound")
nope.Name = "nope"
nope.SoundId = "rbxassetid://116261678102978"
nope.PlaybackSpeed = 0.13
nope.Parent = Chat_
nope.Volume = 10

local nope2 = Instance.new("Sound")
nope2.Name = "nope2"
nope2.SoundId = "rbxassetid://9045389581"
nope2.Parent = Chat_
nope2.Volume = 0

local static = Instance.new("Sound")
static.Name = "static"
static.SoundId = "rbxassetid://5547866547"
static.Parent = Chat_
static.Volume = 7

local function giveGUI(player)
	local guiClone = Chat_:Clone()
	guiClone.Parent = player:WaitForChild("PlayerGui")

	guiClone.TextLabel.Text = ""
	guiClone.obama.Visible = true
	guiClone.nope:Play()

	task.wait(3.8)

	for i = 1, 10 do
		guiClone.obama.ImageTransparency += 0.1
		task.wait(0.05)
	end

	guiClone.obama.ZIndex = 0

	task.wait(7.5)

	for i = 1, 10 do
		guiClone.obunga.ImageTransparency -= 0.1
		task.wait(0.01)
	end

	guiClone.help.ImageTransparency = 0
	guiClone.nope2:Play()

	task.spawn(function()
		while guiClone.nope2.Playing do
			task.wait(math.random(1, 7))
			guiClone.static:Play()
			guiClone.help.ZIndex = 2
			task.wait(0.3)
			guiClone.help.ZIndex = 0
			guiClone.static:Stop()
		end
	end)
end

for _, player in ipairs(Players:GetPlayers()) do
	giveGUI(player)
end

Players.PlayerAdded:Connect(giveGUI)
    ]],

    ["Trump face.txt"] = [[
-- Trump Face LUA
-- the orange guy
-- made by Larkinian
















 

local Players = game:GetService("Players")

local IMAGE_ID = "http://www.roblox.com/asset/?id=1496162497"

local function aplicar(player, character)
	local head = character:WaitForChild("Head", 5)
	if not head then return end

	if head:FindFirstChild("HeadGui") then
		head.HeadGui:Destroy()
	end

	head.Transparency = 1
	head.CanCollide = false

	local gui1 = Instance.new("BillboardGui")
	gui1.Name = "HeadGui"
	gui1.Parent = head
	gui1.Adornee = head
	gui1.Size = UDim2.new(2.5, 0, 2.5, 0)
	gui1.StudsOffset = Vector3.new(0, 0.2, 0)
	gui1.AlwaysOnTop = true

	local img = Instance.new("ImageLabel")
	img.Name = "Image"
	img.Image = IMAGE_ID
	img.Size = UDim2.new(1, 0, 1, 0)
	img.Position = UDim2.new(0, 0, 0, 0)
	img.BackgroundTransparency = 1
	img.Parent = gui1
end

for _, player in pairs(Players:GetPlayers()) do
	if player.Character then
		aplicar(player, player.Character)
	end
	player.CharacterAdded:Connect(function(char)
		aplicar(player, char)
	end)
end

Players.PlayerAdded:Connect(function(player)
	player.CharacterAdded:Connect(function(char)
		aplicar(player, char)
	end)
end)
    ]],

