local function setPageEnabled(state)
for _, obj in ipairs(mainPage:GetChildren()) do
if obj:IsA("GuiObject") and obj ~= bottomBar then
obj.Visible = state
end
end
end

settingsButton.MouseButton1Click:Connect(function()
setPageEnabled(false)
end)

executorTab.MouseButton1Click:Connect(function()
setPageEnabled(true)
end)

