-- funo criarBotao
local function criarBotao(nome)
	local botao = Instance.new("TextButton")
	botao.Size = UDim2.new(1, 0, 0, 18)
	botao.BackgroundColor3 = Color3.fromRGB(255,255,255)
	botao.BorderSizePixel = 0
	botao.TextSize = 9
	botao.TextColor3 = Color3.fromRGB(0,0,0)
	botao.TextXAlignment = Enum.TextXAlignment.Left
	botao.Text = "   "..nome
	botao.AutoButtonColor = false
	botao.Parent = sideScroll
	
	-- Hover azul
	botao.MouseEnter:Connect(function()
		botao.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
		botao.TextColor3 = Color3.fromRGB(255,255,255)
	end)

	botao.MouseLeave:Connect(function()
		botao.BackgroundColor3 = Color3.fromRGB(255,255,255)
		botao.TextColor3 = Color3.fromRGB(0,0,0)
	end)

	--  Quando clicar coloca o cdigo na TextBox
	botao.MouseButton1Click:Connect(function()
		local codigo = scripts[nome]
		if codigo then
			codeBox.Text = codigo
		else
			codeBox.Text = ""
		end
	end)

	return botao
end

